{
	"name": "physi",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"proto:generate": "rm -rf proto/generated && mkdir -p proto/generated/ts/proto && protoc --ts_out=proto/generated/ts/proto/ --ts_opt eslint_disable,long_type_string,ts_nocheck --proto_path=proto/src/ proto/src/third_party/*.proto  proto/src/google/protobuf/*.proto",
		"proto:format": "clang-format --style=google -i proto/src/*.proto"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@protobuf-ts/plugin": "^2.9.4",
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/node": "^22.10.7",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^5.4.11",
		"vitest": "^2.1.8",
		"zod": "^3.24.1"
	}
}